{
  "name": "@social-hustle/utils-cache",
  "version": "1.1.0",
  "description": "Ready-to-use implementations of Xiphe's cachified",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./lru": {
      "types": "./dist/types/lru/index.d.ts",
      "import": "./dist/esm/lru/index.js",
      "require": "./dist/cjs/lru/index.js"
    },
    "./upstash-redis": {
      "types": "./dist/types/upstash-redis/index.d.ts",
      "import": "./dist/esm/upstash-redis/index.js",
      "require": "./dist/cjs/upstash-redis/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "upstash-redis",
    "lru"
  ],
  "scripts": {
    "build": "tsup",
    "build:types": "tsc -p tsconfig.declarations.json",
    "lint": "eslint",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@upstash/redis": "1.19.1",
    "cachified": "3.5.4",
    "lru-cache": "10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
