{
  "name": "@social-hustle/ui-carousel",
  "version": "1.0.0",
  "description": "Customizable, reusable Framer Motion carousel component",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "./tailwind.plugin.js"
  ],
  "scripts": {
    "build": "tsup",
    "build:css": "npx tailwindcss -i ./src/styles/index.css -o ./public/compiled/tailwind.css",
    "build:css:watch": "pnpm run build:css --watch",
    "build:storybook": "storybook build",
    "dev": "tsup --watch",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "eslint",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "tsc": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "framer-motion": "^10.10.0"
  },
  "peerDependencies": {
    "framer-motion": "^10.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "tailwindcss": "3.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
