{
  "name": "@social-hustle/ui-form",
  "version": "1.0.0",
  "description": "Form component for React applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/tailwind"
  ],
  "scripts": {
    "build": "tsup",
    "build:css": "npx tailwindcss -o ./public/compiled/tailwind.css",
    "build:css:watch": "pnpm run build:css --watch",
    "build:storybook": "storybook build",
    "dev": "tsup --watch",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "eslint",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@social-hustle/utils-classnames": "workspace:*",
    "@ts-react/form": "1.8.1",
    "react-hook-form": "7.47.0",
    "tailwindcss": "3.3.2",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "@hookform/resolvers": "3.3.2",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@social-hustle/utils-classnames": "workspace:*",
    "@ts-react/form": "1.8.1",
    "react-hook-form": "7.47.0",
    "tailwindcss": "3.3.2",
    "zod": "3.22.4"
  }
}
